				  <option>MediumFlatRateBox</option>
				  <option>LargeFlatRateBox</option>
				</select>
			</div>
	</div>

	<div>
		<h4>service:</h4>
	</div>
	<div class="form-group" style="margin-bottom: 1em" >
		<div class="btn-group">
		  <button type="button" class="btn btn-danger">Priority Mail</button>
		  <button type="button" class="btn btn-primary">Express Mail</button>
		</div>
	</div>

